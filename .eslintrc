{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 2],
    "semi": "off",
    "linebreak-style": ["error", "windows"],
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "object-curly-spacing" : "off",
    "eol-last": "off",
    "no-trailing-spaces": "off",
    "operator-linebreak": "off",
    "no-debugger": "off",
    "no-unused-vars":"off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}